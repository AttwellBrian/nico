{
  "name": "rosetta",
  "version": "0.3.0",
  "description": "Front-end application scaffold",
  "private": true,
  "dependencies": {
    "@css/css": "0.34.0",
    "@css/truman": "0.14.0",
    "@css/web-core": "0.18.0",
    "@css/web-types": "0.44.0",
    "@types/history": "4.7.2",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.2",
    "history": "4.9.0",
    "moment": "2.24.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "request-promise-native": "1.0.7",
    "reselect": "4.0.0",
    "semantic-ui-react": "0.87.3",
    "typescript": "3.4.3",
    "use-interval": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src/**/*.{ts,tsx}",
    "build": "npm install && react-scripts build && mkdir -p dist && rm -rf dist/release/build && mv build dist/release",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/request-promise": "4.1.44",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,css}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
